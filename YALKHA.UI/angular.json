{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "yalkha.ui": {
      "root": "",
      "sourceRoot": "./",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "libs",
            "index": "index.html",
            "main": "main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "polyfills.ts",
            "assets": [
              "favicon.ico",
              "assets"
            ],
            "styles": [
              "./node_modules/primeicons/primeicons.css",
              "./node_modules/primeng/resources/themes/saga-blue/theme.css",
              "./node_modules/primeng/resources/primeng.min.css",
              "./node_modules/loaders.css/loaders.min.css",
              "./assets/css/primeng-style.css",
              "./assets/css/styles.css"
            ],
            "scripts": [
              "./node_modules/quill/dist/quill.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "yalkha.ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "yalkha.ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "yalkha.ui:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "yalkha.ui",
  "cli": {
    "analytics": "adf5d5b1-c137-40bd-bf77-d55545fb2b5b"
  }
}